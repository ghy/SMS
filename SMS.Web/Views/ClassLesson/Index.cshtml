@model SMS.Web.Models.ClassLesson.IndexViewModel
@section Title{
课表
}
@section SiteHead{
}
@section SiteMap{
&gt;&gt; 课表
}
@section SitePageName{
课表
}
<div class="context">
    @if (Model.ClassLessons.Count == 0)
    {
        @Html.NoRecord()
    }
    else
    {
        <table border="0px" bordercolor="#eaeaea" class="dataTable">
            @for (int i = 0; i < Model.ClassLessons.Count; i++)
            {
                <tr>
                    <td>@(i + 1)
                    </td>
                </tr>
                <tr>
                    <td>@Model.ClassLessons[i].Name
                    </td>
                </tr>
            }
        </table>
    }
</div>
